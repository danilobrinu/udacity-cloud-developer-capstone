{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Create Todo Request Schema",
  "type": "object",
  "properties": {
    "item": {
      "type": "object",
      "properties": {
        "todoId": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "dueDate": {
          "type": "string"
        },
        "done": {
          "type": "boolean"
        },
        "attachmentUrl": {
          "type": "string"
        }
      },
      "required": ["todoId", "createdAt", "name", "dueDate", "done"]
    }
  },
  "required": ["item"]
}
