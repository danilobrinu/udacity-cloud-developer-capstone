{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Get Todos Response Schema",
  "type": "object",
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "todoId": {
            "type": "string"
          },
          "createdAt": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "dueDate": {
            "type": "string"
          },
          "done": {
            "type": "boolean"
          },
          "attachmentUrl": {
            "type": "string"
          }
        },
        "required": ["todoId", "createdAt", "name", "dueDate", "done"]
      }
    }
  },
  "required": ["uploadUrl"]
}
