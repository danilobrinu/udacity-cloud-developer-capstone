{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Create Todo Request Schema",
  "type": "object",
  "properties": {
    "body": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "minLength": 3
        },
        "dueDate": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": ["name", "dueDate"]
    }
  }
}
